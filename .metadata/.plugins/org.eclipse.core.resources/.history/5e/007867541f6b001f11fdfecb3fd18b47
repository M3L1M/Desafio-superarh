package com.resistores;

import java.util.Scanner;

public class Resistores {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		int ohm = scanner.nextInt();
		retornar(ohm);
		
		if(ohm<1000) {
			System.out.println(ohm+" ohms");
		}else if(ohm>=1000 && ohm<1000000) {
			System.out.println((ohm/1000)+"K ohms");
		}else {
			System.out.println((ohm/1000000)+"M ohms");
		}
		
	}
	
	public static String retornar(int ohm) {
		String ohms= ohm+"";
		String[] cores = {
				"preto","marrom","vermelho","laranja","amarelo",
				"verde","azul","violeta","cinza","branco"
		};
		
		ohms=ohms.substring(1,2);
		System.out.println("-"+ohms);
		
		//0: preto
		//1: marrom
		//2: vermelho
		//3: laranja
		//4: amarelo
		//5: verde
		//6: azul
		//7: violeta
		//8: cinza
		//9: branco
		
		return "";
	}
}
